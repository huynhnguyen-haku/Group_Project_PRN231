@using MilkData.Models
@{
    ViewData["Title"] = "Add";
}

@model Account

<div class="form-group row">
    <div class="col-md-6">
        @Html.HiddenFor(model => model.AccountId)
    </div>
</div>
<div class="form-group row">
    <div class="col-md-6">
        <label for="1">UserName:</label>
        @Html.EditorFor(model => model.UserName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">FullName:</label>
        @Html.EditorFor(model => model.FullName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Email:</label>
        @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Password:</label>
        @Html.EditorFor(model => model.Password, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Phone:</label>
        @Html.EditorFor(model => model.Phone, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Address:</label>
        @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Role:</label>
        @Html.EditorFor(model => model.Role, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">Point:</label>
        @Html.EditorFor(model => model.Point, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">AvatarUrl:</label>
        @Html.EditorFor(model => model.AvatarUrl, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="col-md-6">
        <label for="1">IsActive</label>
        @Html.CheckBoxFor(model => model.IsActive, new { htmlAttributes = new { @class = "form-check-input" } })
    </div>
</div>
